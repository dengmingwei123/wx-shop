page {
  background-color: #eeeeee;
}
.feedback .fb_main {
  padding: 20rpx;
  color: #666;
}
.feedback .fb_main .fb_title {
  font-size: 26rpx;
  color: #777;
}
.feedback .fb_main .fb_tips {
  display: flex;
  flex-wrap: wrap;
}
.feedback .fb_main .fb_tips text {
  width: 30%;
  padding: 10rpx 0;
  margin: 10rpx;
  background-color: #fff;
  text-align: center;
}
.feedback .fb_main .fb_content {
  background-color: #fff;
  margin-top: 20rpx;
  padding: 10rpx;
}
.feedback .fb_main .fb_content textarea {
  width: 100%;
}
.feedback .fb_main .fb_content .fb_tool {
  display: flex;
  flex-wrap: wrap;
}
.feedback .fb_main .fb_content .fb_tool button {
  width: 100rpx;
  height: 100rpx;
  padding: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 25rpx 0 0 20rpx;
  font-weight: bold;
  color: #ccc;
  font-size: 50rpx;
  border-radius: 10rpx;
}
.feedback .fb_main .fb_content .fb_tool .up_img_item {
  margin-left: 25rpx;
  margin-top: 25rpx;
}
.feedback .fb_main .form_btn_wrap {
  margin-top: 20rpx;
  display: flex;
  justify-content: flex-end;
}
.feedback .fb_main .form_btn_wrap button {
  margin: 0;
  width: 30%;
  display: flex;
  align-items: center;
  justify-content: center;
}
